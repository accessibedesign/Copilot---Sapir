{
  "name": "@acsbe/accesswidget-ui",
  "version": "1.0.0",
  "description": "A ui library written using preact for the `accesswidget` project.",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.module.d.ts",
      "import": "./dist/esm/index.module.js",
      "require": "./dist/cjs/index.module.js",
      "default": "./dist/esm/index.module.js"
    },
    "./types": {
      "types": "./dist/types/types/exports.d.ts",
      "import": "./dist/esm/types/exports.js",
      "require": "./dist/cjs/types/exports.js",
      "default": "./dist/esm/types/exports.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.module.d.ts"
      ],
      "types": [
        "./dist/types/types/exports.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "release": {
    "analyzeCommits": "semantic-release-conventional-commits",
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "assets": "dist/*.tgz"
        }
      ]
    ]
  },
  "scripts": {
    "prepare": "npm run build && husky install",
    "serve": "NODE_OPTIONS=--openssl-legacy-provider preact watch",
    "clean": "rimraf dist",
    "build:esm": "tsc -p ./tsconfig.esm.json && node scripts/build/index.js && tsc-alias -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json && node scripts/build/index.js && tsc-alias -p ./tsconfig.cjs.json",
    "build:types": "tsc -p ./tsconfig.types.json && tsc-alias -p ./tsconfig.types.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "deploy": "npx semantic-release",
    "lint": "eslint '{src,tests}/**/*.{ts,tsx}'",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook build",
    "audit:vulnerability": "npm audit --audit-level=high --omit=dev",
    "test:visual-regression": "NODE_OPTIONS=--openssl-legacy-provider npx chromatic",
    "test:coverage": "jest --coverage --silent=\"false\"",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acsbe/accessWidget-UI.git"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "preact",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "build/"
    ]
  },
  "author": {
    "name": "Adi Darachi",
    "email": "adida@accessibe.com"
  },
  "dependencies": {
    "preact": "^10.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@chromatic-com/storybook": "^1.5.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@semantic-release/exec": "^6.0.3",
    "@storybook/addon-essentials": "^8.1.10",
    "@storybook/addon-interactions": "^8.1.10",
    "@storybook/addon-links": "^8.1.10",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/preact": "^8.1.10",
    "@storybook/preact-webpack5": "^8.1.10",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/test": "^8.1.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/preact": "^3.2.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.3.0",
    "babel-preset-preact": "^2.0.0",
    "chromatic": "^11.5.4",
    "commitlint": "^19.1.0",
    "construct-style-sheets-polyfill": "^3.1.0",
    "css-loader": "^7.1.2",
    "csso": "^5.0.5",
    "eslint": "^7.32.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-preset-preact": "^4.0.2",
    "jest-watch-typeahead": "^2.2.2",
    "postcss-loader": "^8.1.1",
    "preact-cli": "^3.0.5",
    "preact-render-to-string": "^5.1.11",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.1",
    "sass": "^1.57.1",
    "sass-loader": "^10.4.1",
    "semantic-release": "^20.1.1",
    "semantic-release-conventional-commits": "^3.0.0",
    "shadow-dom-testing-library": "^1.10.0",
    "storybook": "^8.1.10",
    "storybook-addon-fetch-mock": "^2.0.0",
    "style-loader": "^4.0.0",
    "svgo": "^3.0.2",
    "ts-jest": "^27.1.5",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.1.3",
    "webpack": "^4.47.0"
  },
  "readme": "ERROR: No README data found!"
}